<ResourceDictionary
	            xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	            xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Einträge ins SymbolResourceDictionary sollten hier erfolgen -->
    <DataTemplate x:Key="ReportingArrowLeft">
        <Image Source="/Resources/Images/Reporting/reporting.arrow.left.png" Stretch="Fill" Width="40" Height="40" />
    </DataTemplate>
    <DataTemplate x:Key="ReportingArrowLeftM">
        <Image Source="/Resources/Images/Reporting/reporting.arrow.leftm.png" Stretch="Fill" Width="40" Height="40" />
    </DataTemplate>
    <DataTemplate x:Key="ReportingArrowRight">
        <Image Source="/Resources/Images/Reporting/reporting.arrow.rigt.png" Stretch="Fill" Width="40" Height="40" />
