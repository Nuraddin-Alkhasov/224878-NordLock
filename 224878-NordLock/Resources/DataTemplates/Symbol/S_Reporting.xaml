    <DataTemplate x:Key="ReportingArrowRightM">
        <Image Source="/Resources/Images/Reporting/reporting.arrow.rigtm.png" Stretch="Fill" Width="40" Height="40" />
    </DataTemplate>
    <DataTemplate x:Key="ReportingPrint">
        <Image Source="/Resources/Images/Reporting/reporting.printer.png" Stretch="Fill" Width="40" Height="40" />
    </DataTemplate>
    <DataTemplate x:Key="ReportingLayout">
        <Image Source="/Resources/Images/Reporting/reporting.layout.png" Stretch="Fill" Width="40" Height="40" />
    </DataTemplate>
    <DataTemplate x:Key="ReportingCog">
        <Image Source="/Resources/Images/Reporting/reporting.cog.png" Stretch="Fill" Width="40" Height="40" />
