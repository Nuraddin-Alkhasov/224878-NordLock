<UserControl x:Class="HMI.UserControls.MV_Shalves"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vw="http://inosoft.com/visiwin7" >
    <Grid x:Name="A" Opacity="0" Loaded="UserControl_Loaded">
        <vw:PictureBox SymbolResourceKey="Shelves" Height="121" Margin="4,3,29,3" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <vw:PictureBox x:Name="QSDoor1" SymbolResourceKey="QSDoorOpen" Width="75" Height="16" Margin="7,80,0,0" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <vw:PictureBox x:Name="QSDoor2" SymbolResourceKey="QSDoorClosed" Width="75" Height="16" Margin="7,68,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" />
        <vw:Button x:Name="lr" Height="80" Width="100" HorizontalAlignment="Right" VerticalAlignment="Bottom"  Style="{DynamicResource B_Gray_ns}" Background="{DynamicResource FP_Yellow_Gradient}" BlinkBrush="{DynamicResource FP_Gray_Gradient}" LocalizableText="@MainView.Text10" IsEnabled="False" Opacity="1" Margin="0,0,37,-80"/>
        <vw:Button x:Name="qs" Height="80" Width="140" HorizontalAlignment="Left" VerticalAlignment="Bottom"  Style="{DynamicResource B_Gray_ns}" Background="{DynamicResource FP_LightGreen_Gradient}" LocalizableText="@MainView.Text66" IsEnabled="False" Opacity="1" Margin="-8,0,0,-80"/>

    </Grid>

</UserControl>
