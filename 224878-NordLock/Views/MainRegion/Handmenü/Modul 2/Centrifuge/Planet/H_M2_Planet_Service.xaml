         xmlns:UC="clr-namespace:HMI.UserControls"
         Width="1840" Height="890" Background="{DynamicResource MainViewBackgroundBrush}">

    <UC:H_Service
        StepPositive="NL.PLC.Blocks.2 Modul 2.10 Zentrifuge.5 Planet.DB BS Planet HMI.PC.Drive.Tippen +"
