        ActualValue="NL.PLC.Blocks.2 Modul 2.10 Zentrifuge.5 Planet.DB BS Planet HMI.Istwerte.Ist Geschwindigkeit"
        ActualValueUnit="@Units.Rpm"
        SetReferenceVar="NL.PLC.Blocks.2 Modul 2.10 Zentrifuge.5 Planet.DB BS Planet HMI.PC.Drive.Vorwahl Referenz"
        ReferenceLogText="@Logging.Service.Text6"
        DeleteReferenceVar="NL.PLC.Blocks.2 Modul 2.10 Zentrifuge.5 Planet.DB BS Planet HMI.PC.General.Refernez löschen"
        DelReferenceLogText="@Logging.Service.Text16"/>
